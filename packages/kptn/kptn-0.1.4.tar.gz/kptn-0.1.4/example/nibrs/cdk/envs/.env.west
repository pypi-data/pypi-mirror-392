# AWS Variables (credentials in $HOME/.aws/credentials)
AWS_DEFAULT_REGION=us-west-1

# suffix is to differentiate resources between stacks like S3 buckets and IAM user
RSRC_SUFFIX=west

# All stacks source the same externals, artifacts stored per stack
ARTIFACTS_BUCKET_NAME=nibrs-estimation-artifacts

# Key pair for ssh access to ec2
KEY_PAIR_NAME=nibrs-estimation-aws-west

# If importing existing EFS, set the EFS id here
NIBRS_EFS_ID=