{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$ref": "#/FWQuantity",
  "FWQuantity": {
    "type": "object",
    "additionalProperties": false,
    "properties": {
      "_fw_name": {
        "const": "{{virtmat.language.utilities.serializable.FWQuantity}}"
      },
      "_version": {
        "$ref": "vmschemaversion.json#/SchemaVersion"
      },
      "data": {
        "type": "array",
        "items": [
          {
            "$ref": "#/definitions/Numerical"
          },
          {
            "$ref": "vmgeneric.json#/TupleUnits"
          }
        ],
        "additionalItems": false
      }
    },
    "required": [
      "_fw_name",
      "data"
    ]
  },
  "definitions": {
    "Numerical": {
      "anyOf": [
        {
          "type": "number"
        },
        {
          "const": null
        },
        {
          "$ref": "#/definitions/FWUFloat"
        },
        {
          "$ref": "vmgeneric.json#/TupleComplex"
        }
      ]
    },
    "FWUFloat": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "_fw_name": {
          "const": "{{virtmat.language.utilities.serializable.FWUFloat}}"
        },
        "_version": {
          "$ref": "vmschemaversion.json#/SchemaVersion"
        },
        "n": {
          "type": "number"
        },
        "s": {
          "type": "number"
        }
      }
    }
  }
}
