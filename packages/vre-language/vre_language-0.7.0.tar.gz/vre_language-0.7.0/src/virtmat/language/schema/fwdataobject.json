{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$ref": "#/FWDataObject",
  "FWDataObject": {
    "type": "object",
    "additionalProperties": false,
    "properties": {
      "_fw_name": {
        "const": "{{virtmat.language.utilities.serializable.FWDataObject}}"
      },
      "_version": {
        "$ref": "vmschemaversion.json#/SchemaVersion"
      },
      "datastore": {
        "type": "object",
        "properties": {
          "type": {
            "enum": [
              "file",
              "url",
              "gridfs",
              null
            ]
          },
          "path": {
            "$ref": "generic.json#/AbsoluteFilePath"
          },
          "format": {
            "enum": [
              "json",
              "yaml",
              "hdf5",
              "custom"
            ],
            "default": "json"
          },
          "name": {
            "$ref": "generic.json#/Identifier"
          },
          "launchpad": {
            "oneOf": [
              {
                "$ref": "generic.json#/AbsoluteFilePath"
              },
              {
                "const": null
              }
            ]
          },
          "compress": {
            "type": "boolean",
            "default": true
          },
          "inline-threshold": {
            "type": "integer",
            "minimum": 0,
            "default": 100000
          }
        },
        "additionalProperties": false,
        "required": [
          "type"
        ]
      },
      "filename": {
        "$ref": "generic.json#/FilePath"
      },
      "url": {
        "$ref": "vmgeneric.json#/URI"
      },
      "value": {
        "$ref": "fwdataobjectvalue.json#/FWDataObjectValue"
      }
    },
    "required": [
      "_fw_name",
      "datastore"
    ]
  }
}
