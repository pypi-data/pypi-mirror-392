    sh:property [ sh:class gocam:TermObject ;
            sh:nodeKind sh:IRI ;
            sh:order 26 ;
            sh:path gocam:annoton_terms ],
        [ sh:class gocam:TermObject ;
            sh:description "The reflexive transitive closure of `model_activity_molecular_function_terms`, over the is_a relationship" ;
            sh:nodeKind sh:IRI ;
            sh:order 8 ;
            sh:path gocam:model_activity_molecular_function_closure ],
        [ sh:class gocam:Activity ;
            sh:description "The set of activities that are the starting points of the model, i.e. those that have no incoming causal associations." ;
            sh:nodeKind sh:IRI ;
            sh:order 27 ;
            sh:path gocam:start_activities ],
        [ sh:class gocam:TermObject ;
            sh:description "All direct enabler genes and genes that are part of enabler complexes for all activities" ;
            sh:nodeKind sh:IRI ;
            sh:order 16 ;
            sh:path gocam:model_activity_enabled_by_genes ],
        [ sh:class gocam:TermObject ;
            sh:description "The reflexive transitive closure of the taxon term for the model, over the NCBITaxon:subClassOf relationship type. This is used to determine the set of taxa that are relevant to the model." ;
            sh:nodeKind sh:IRI ;
            sh:order 24 ;
            sh:path gocam:model_taxon_closure ],
        [ sh:datatype xsd:integer ;
            sh:description "Total number of causal association edges connecting activities in a model." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 3 ;
            sh:path gocam:number_of_causal_associations ],
        [ sh:datatype xsd:string ;
            sh:description "The label of the primary taxon for the model" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path gocam:taxon_label ],
        [ sh:datatype xsd:integer ;
            sh:description "The maximum number of hops along activities along the direction of causal flow in a model." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 4 ;
            sh:path gocam:length_of_longest_causal_association_path ],
        [ sh:class gocam:Activity ;
            sh:description "The set of activities that are the end points of the model, i.e. those that have no outgoing causal associations." ;
            sh:nodeKind sh:IRI ;
            sh:order 28 ;
            sh:path gocam:end_activities ],
        [ sh:class gocam:TermObject ;
            sh:description "The primary taxon term for the model, over the NCBITaxon:subClassOf relationship type. This is used to determine the primary taxon that the model is relevant to." ;
            sh:nodeKind sh:IRI ;
            sh:order 23 ;
            sh:path gocam:model_taxon ],
        [ sh:class gocam:TermObject ;
            sh:description "The rollup of the taxon closure to a NCBITaxon subset or slim." ;
            sh:nodeKind sh:IRI ;
            sh:order 25 ;
            sh:path gocam:model_taxon_rollup ],
        [ sh:datatype xsd:integer ;
            sh:description "The number of molecular entities or sets of entities in a model." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 2 ;
            sh:path gocam:number_of_enabled_by_terms ],
        [ sh:class gocam:TermObject ;
            sh:description "All direct biological process terms for all activities" ;
            sh:nodeKind sh:IRI ;
            sh:order 17 ;
            sh:path gocam:model_activity_part_of_terms ],
        [ sh:class gocam:TermObject ;
            sh:description "All MF terms for all activities" ;
            sh:nodeKind sh:IRI ;
            sh:order 7 ;
            sh:path gocam:model_activity_molecular_function_terms ],
        [ sh:class gocam:TermObject ;
            sh:description "The reflexive transitive closure of `model_activity_occurs_in_terms`, over the is_a and part_of relationship type" ;
            sh:nodeKind sh:IRI ;
            sh:order 11 ;
            sh:path gocam:model_activity_occurs_in_closure ],
        [ sh:class gocam:TermObject ;
            sh:description "All direct cellular component localization terms for all activities" ;
            sh:nodeKind sh:IRI ;
            sh:order 10 ;
            sh:path gocam:model_activity_occurs_in_terms ],
        [ sh:datatype xsd:integer ;
            sh:description "The number of distinct components that consist of activities that are connected (directly or indirectly) via causal connections. Most models will consist of a single SCC. Some models may consist of two or more \"islands\" where there is no connection from one island to another." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 5 ;
            sh:path gocam:number_of_strongly_connected_components ],
        [ sh:class gocam:TermObject ;
            sh:description "All direct enabler terms for all activities" ;
            sh:nodeKind sh:IRI ;
            sh:order 13 ;
            sh:path gocam:model_activity_enabled_by_terms ],
        [ sh:class gocam:PublicationObject ;
            sh:description "All publication objects from the model across different levels combined in one place" ;
            sh:nodeKind sh:IRI ;
            sh:order 6 ;
            sh:path gocam:flattened_references ],
        [ sh:datatype xsd:integer ;
            sh:description "The number of end activities in a model" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 32 ;
            sh:path gocam:number_of_end_activities ],
        [ sh:class gocam:TermObject ;
            sh:description "The reflexive transitive closure of `model_activity_enabled_by_terms`, over the is_a and part_of relationship types" ;
            sh:nodeKind sh:IRI ;
            sh:order 14 ;
            sh:path gocam:model_activity_enabled_by_closure ],
        [ sh:class gocam:TermObject ;
            sh:description "The rollup of `model_activity_has_input_closure` to a GO subset or slim." ;
            sh:nodeKind sh:IRI ;
            sh:order 22 ;
            sh:path gocam:model_activity_has_input_rollup ],
        [ sh:datatype xsd:integer ;
            sh:description "The number of intermediate activities in a model" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 33 ;
            sh:path gocam:number_of_intermediate_activities ],
        [ sh:datatype xsd:integer ;
            sh:description "The number of singleton activities in a model" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 34 ;
            sh:path gocam:number_of_singleton_activities ],
        [ sh:class gocam:TermObject ;
            sh:description "The reflexive transitive closure of `model_activity_has_input_terms`, over the is_a relationship type" ;
            sh:nodeKind sh:IRI ;
            sh:order 21 ;
            sh:path gocam:model_activity_has_input_closure ],
        [ sh:class gocam:Activity ;
            sh:description "The set of activities that have no causal associations, i.e. those that are not connected to any other activity in the model." ;
            sh:nodeKind sh:IRI ;
            sh:order 30 ;
            sh:path gocam:singleton_activities ],
        [ sh:class gocam:TermObject ;
            sh:description "The rollup of `model_activity_molecular_function_closure` to a GO subset or slim." ;
            sh:nodeKind sh:IRI ;
            sh:order 9 ;
            sh:path gocam:model_activity_molecular_function_rollup ],
        [ sh:datatype xsd:integer ;
            sh:description "The number of start activities in a model" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 31 ;
            sh:path gocam:number_of_start_activities ],
        [ sh:class gocam:TermObject ;
            sh:description "The rollup of `model_activity_occurs_in_closure` to a GO subset or slim." ;
            sh:nodeKind sh:IRI ;
            sh:order 12 ;
            sh:path gocam:model_activity_occurs_in_rollup ],
        [ sh:class gocam:TermObject ;
            sh:description "The rollup of `model_activity_part_of_closure` to a GO subset or slim." ;
            sh:nodeKind sh:IRI ;
            sh:order 19 ;
            sh:path gocam:model_activity_part_of_rollup ],
        [ sh:class gocam:TermObject ;
            sh:description "All direct input terms for all activities" ;
            sh:nodeKind sh:IRI ;
            sh:order 20 ;
            sh:path gocam:model_activity_has_input_terms ],
        [ sh:class gocam:TermObject ;
            sh:description "The rollup of `model_activity_enabled_by_closure` to a GO subset or slim." ;
            sh:nodeKind sh:IRI ;
            sh:order 15 ;
            sh:path gocam:model_activity_enabled_by_rollup ],
        [ sh:class gocam:Activity ;
            sh:description "The set of activities that are neither start nor end activities, i.e. those that have both incoming and outgoing causal associations." ;
            sh:nodeKind sh:IRI ;
            sh:order 29 ;
            sh:path gocam:intermediate_activities ],
        [ sh:datatype xsd:integer ;
            sh:description "The number of activities in a model." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path gocam:number_of_activities ],
        [ sh:class gocam:TermObject ;
            sh:description "The reflexive transitive closure of `model_activity_part_of_terms`, over the is_a and part_of relationship type" ;
            sh:nodeKind sh:IRI ;
            sh:order 18 ;
            sh:path gocam:model_activity_part_of_closure ] ;
    sh:targetClass gocam:QueryIndex .

gocam:BiologicalProcessAssociation a sh:NodeShape ;
