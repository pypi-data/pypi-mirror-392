@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix gocam: <https://w3id.org/gocam/> .
@prefix pav: <http://purl.org/pav/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

gocam:Association a sh:NodeShape ;
    sh:closed false ;
    sh:description "An abstract grouping for different kinds of evidence-associated provenance" ;
    sh:ignoredProperties ( gocam:members gocam:downstream_activity gocam:part_of rdf:type gocam:term gocam:happens_during gocam:predicate ) ;
    sh:property [ sh:datatype xsd:string ;
            sh:description "The type of association." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path gocam:type ],
        [ sh:class gocam:ProvenanceInfo ;
            sh:description "The set of provenance objects that provide metadata about who made the association." ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 2 ;
            sh:path gocam:provenances ],
        [ sh:class gocam:EvidenceItem ;
            sh:description "The set of evidence items that support the association." ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 1 ;
            sh:path gocam:evidence ] ;
    sh:targetClass gocam:Association .

gocam:EnabledByGeneProductAssociation a sh:NodeShape ;
    sh:closed true ;
    sh:description "An association between an activity and an individual gene product" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class gocam:GeneProductTermObject ;
            sh:description "A \"term\" that is an entity database object representing an individual gene product." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:IRI ;
            sh:order 0 ;
            sh:path gocam:term ],
        [ sh:class gocam:EvidenceItem ;
            sh:description "The set of evidence items that support the association." ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 2 ;
            sh:path gocam:evidence ],
        [ sh:datatype xsd:string ;
            sh:description "The type of association." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path gocam:type ],
        [ sh:class gocam:ProvenanceInfo ;
            sh:description "The set of provenance objects that provide metadata about who made the association." ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 3 ;
            sh:path gocam:provenances ] ;
    sh:targetClass gocam:EnabledByGeneProductAssociation .

gocam:EnabledByProteinComplexAssociation a sh:NodeShape ;
    sh:closed true ;
    sh:description "An association between an activity and a protein complex, where the complex carries out the activity. This should only be used when the activity cannot be attributed to an individual member of the complex, but instead the function is an emergent property of the complex." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class gocam:ProvenanceInfo ;
            sh:description "The set of provenance objects that provide metadata about who made the association." ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 4 ;
            sh:path gocam:provenances ],
        [ sh:class gocam:EvidenceItem ;
            sh:description "The set of evidence items that support the association." ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 3 ;
            sh:path gocam:evidence ],
        [ sh:datatype xsd:string ;
            sh:description "The type of association." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 2 ;
            sh:path gocam:type ],
        [ sh:class gocam:GeneProductTermObject ;
            sh:description "The gene products that are part of the complex" ;
            sh:nodeKind sh:IRI ;
            sh:order 0 ;
            sh:path gocam:members ],
        [ sh:class gocam:ProteinComplexTermObject ;
            sh:description "The gene product or complex that carries out the activity" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:IRI ;
            sh:order 1 ;
            sh:path gocam:term ] ;
    sh:targetClass gocam:EnabledByProteinComplexAssociation .

gocam:Model a sh:NodeShape ;
    sh:closed true ;
    sh:description "A model of a biological program consisting of a set of causally connected activities." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype xsd:anyURI ;
            sh:description "The identifier of the model. Should be in gocam namespace." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path gocam:id ],
        [ sh:datatype xsd:string ;
            sh:description "The human-readable descriptive title of the model" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path dcterms:title ],
        [ sh:class gocam:TaxonTermObject ;
            sh:description "Additional taxa that the model is about" ;
            sh:nodeKind sh:IRI ;
            sh:order 3 ;
            sh:path gocam:additional_taxa ],
        [ sh:class gocam:Object ;
            sh:description "All of the objects that are part of the model. This includes terms as well as publications and database objects like gene. This is not strictly part of the data managed by the model, it is for convenience, and should be refreshed from outside." ;
            sh:nodeKind sh:IRI ;
            sh:order 8 ;
            sh:path gocam:objects ],
        [ sh:class gocam:QueryIndex ;
            sh:description "An optional object that contains the results of indexing a model with various summary statistics and retrieval indices." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 10 ;
            sh:path gocam:query_index ],
        [ sh:datatype xsd:string ;
            sh:description "Curator-provided comments about the model" ;
            sh:nodeKind sh:Literal ;
            sh:order 6 ;
            sh:path rdfs:comment ],
        [ sh:class gocam:TaxonTermObject ;
            sh:description "The primary taxon that the model is about" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:IRI ;
            sh:order 2 ;
            sh:path gocam:taxon ],
        [ sh:datatype xsd:string ;
            sh:description "The date that the model was last modified" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 5 ;
            sh:path dcterms:date ],
        [ sh:class gocam:ProvenanceInfo ;
            sh:description "Model-level provenance information" ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 9 ;
            sh:path gocam:provenances ],
        [ sh:class gocam:Activity ;
            sh:description "All of the activities that are part of the model" ;
            sh:nodeKind sh:IRI ;
            sh:order 7 ;
            sh:path gocam:activities ],
        [ sh:description "The status of the model in terms of its progression along the developmental lifecycle" ;
            sh:in ( "development" "production" "delete" "review" "internal_test" "closed" ) ;
            sh:maxCount 1 ;
            sh:order 4 ;
            sh:path pav:status ] ;
    sh:targetClass gocam:Model .

gocam:TermAssociation a sh:NodeShape ;
    sh:closed false ;
    sh:description "An association between an activity and a term, potentially with extensions. This is an abstract class for grouping purposes, it should not be directly instantiated, instead a subclass should be instantiated." ;
    sh:ignoredProperties ( gocam:happens_during gocam:part_of rdf:type ) ;
    sh:property [ sh:class gocam:TermObject ;
            sh:description "The ontology term that describes the nature of the association" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:IRI ;
            sh:order 0 ;
            sh:path gocam:term ],
        [ sh:datatype xsd:string ;
            sh:description "The type of association." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path gocam:type ],
        [ sh:class gocam:EvidenceItem ;
            sh:description "The set of evidence items that support the association." ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 2 ;
            sh:path gocam:evidence ],
        [ sh:class gocam:ProvenanceInfo ;
            sh:description "The set of provenance objects that provide metadata about who made the association." ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 3 ;
            sh:path gocam:provenances ] ;
    sh:targetClass gocam:TermAssociation .

