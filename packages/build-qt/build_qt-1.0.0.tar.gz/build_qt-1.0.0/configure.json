        "build_type": "release",
        "features": ["ipc_posix"],
        "build_ohos_abi": "arm64-v8a",
        "build_qt_tag": "v5.15.12-lts-lgpl",
        "build_ohqt_tag": "master",
        "clone_depth": 1,
        "jobs": 4,
        "verbose": false
    },
    "qt-config": {
        "license": "opensource",
        "confirm-license": true,
        "-platform": "win32-g++",
        "-xplatform": "oh-clang",
        "-opengl": "es2",
        "-opengles3": true,
        "-no-dbus": true,
        "openssl-runtime": true,
        "-disable-rpath": true,
        "-nomake": ["tests", "examples"]
