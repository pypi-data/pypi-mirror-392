{
  "parser_name": "ComfyUI Efficiency Nodes",
  "priority": 155,
  "description": "Detects ComfyUI workflows using Efficiency Nodes (Efficient Loader, KSampler Efficient). Works with any model, not just IllustriousXL.",
  "version": "2.0-generic",
  "maintainer": "Ktiseos Nyx",
  "target_file_types": [
    "PNG", "JPG", "JPEG"
  ],
  "detection_rules": [
    {
      "comment": "Rule 1: Must be a valid ComfyUI workflow",
      "condition": "OR",
      "rules": [
        {
          "source_type": "pil_info_key",
          "source_key": "workflow",
          "operator": "is_valid_json"
        },
        {
          "source_type": "pil_info_key",
          "source_key": "prompt",
          "operator": "is_valid_json"
        }
      ]
    },
    {
      "comment": "Rule 2: Must contain Efficiency Nodes (KSampler Efficient, Efficient Loader, etc.)",
      "source_type": "pil_info_key",
      "source_key_options": [
        "workflow",
        "prompt"
      ],
      "json_query_type": "has_any_node_class_type",
      "operator": "is_true",
      "class_types_to_check": [
        "Efficient Loader",
        "KSampler (Efficient)",
        "Eff. Loader SDXL",
        "TSC EfficientLoader",
        "TSC KSampler"
      ]
    }
  ],
  "parsing_instructions": {
    "input_data": {
      "source_options": [
        {
          "source_type": "pil_info_key",
          "source_key": "workflow"
        },
        {
          "source_type": "pil_info_key",
          "source_key": "prompt"
        }
      ],
      "transformations": [
        {
          "type": "json_decode_string_itself"
        }
      ]
    },
    "fields": [
      {
        "comment": "Use targeted extraction for prompts - Efficiency workflows often use String Literal + ImpactWildcardProcessor!",
        "target_key": "prompt",
        "method": "comfyui_targeted_prompt_extraction",
        "target_input": "positive",
        "text_encoder_node_types": [
          "CLIPTextEncode", "Text Multiline", "String Literal", "PrimitiveStringMultiline",
          "ShowText|pysssss", "easy positive", "Text Concatenate", "ImpactWildcardProcessor"
        ]
      },
      {
        "target_key": "negative_prompt",
        "method": "comfyui_targeted_prompt_extraction",
        "target_input": "negative",
        "text_encoder_node_types": [
          "CLIPTextEncode", "Text Multiline", "String Literal", "PrimitiveStringMultiline",
          "ShowText|pysssss", "easy negative", "Text Concatenate", "ImpactWildcardProcessor"
        ]
      },
      {
        "comment": "Extract loader parameters (model, VAE, etc.) from Efficient Loader widgets",
        "target_key": "loader_params",
        "method": "efficiency_extract_loader_params"
      },
      {
        "comment": "Map model name from loader params",
        "target_key": "parameters.model",
        "method": "direct_context_key",
        "context_key": "loader_params.ckpt_name",
        "fallback": "unknown"
      },
      {
        "comment": "Map width from loader params (Efficient Loader stores in widgets)",
        "target_key": "parameters.width",
        "method": "direct_context_key",
        "context_key": "loader_params.empty_latent_width",
        "fallback": 1024
      },
      {
        "comment": "Map height from loader params (Efficient Loader stores in widgets)",
        "target_key": "parameters.height",
        "method": "direct_context_key",
        "context_key": "loader_params.empty_latent_height",
        "fallback": 1024
      },
      {
        "target_key": "parameters.steps",
        "method": "comfy_find_input_of_node_type",
        "node_types": [
          "KSampler (Efficient)",
          "TSC KSampler",
          "KSampler",
          "KSamplerAdvanced"
        ],
        "input_field": "steps",
        "data_type": "integer",
        "fallback": 20
      },
      {
        "target_key": "parameters.cfg_scale",
        "method": "comfy_find_input_of_node_type",
        "node_types": [
          "KSampler (Efficient)",
          "TSC KSampler",
          "KSampler",
          "KSamplerAdvanced"
        ],
        "input_field": "cfg",
        "data_type": "float",
        "fallback": 7.0
      },
      {
        "target_key": "parameters.seed",
        "method": "comfy_find_input_of_node_type",
        "node_types": [
          "KSampler (Efficient)",
          "TSC KSampler",
          "KSampler",
          "KSamplerAdvanced"
        ],
        "input_field": "seed",
        "data_type": "integer",
        "fallback": -1
      },
      {
        "target_key": "parameters.sampler_name",
        "method": "comfy_find_input_of_node_type",
        "node_types": [
          "KSampler (Efficient)",
          "TSC KSampler",
          "KSampler",
          "KSamplerAdvanced"
        ],
        "input_field": "sampler_name",
        "data_type": "string",
        "fallback": "euler"
      },
      {
        "target_key": "parameters.scheduler",
        "method": "comfy_find_input_of_node_type",
        "node_types": [
          "KSampler (Efficient)",
          "TSC KSampler",
          "KSampler",
          "KSamplerAdvanced"
        ],
        "input_field": "scheduler",
        "data_type": "string",
        "fallback": "normal"
      },
      {
        "comment": "Extract raw workflow for reference",
        "target_key": "raw_workflow",
        "method": "comfyui_extract_raw_workflow"
      }
    ],
    "output_template": {
      "parser_name_from_engine": "ComfyUI Efficiency Nodes",
      "tool": "ComfyUI (Efficiency Nodes)",
      "format": "ComfyUI Efficiency Workflow",
      "prompt": "$prompt",
      "negative_prompt": "$negative_prompt",
      "parameters": {
        "model": "$parameters.model",
        "steps": "$parameters.steps",
        "cfg_scale": "$parameters.cfg_scale",
        "seed": "$parameters.seed",
        "sampler_name": "$parameters.sampler_name",
        "scheduler": "$parameters.scheduler",
        "width": "$parameters.width",
        "height": "$parameters.height"
      },
      "architecture_info": {
        "detected_as": "ComfyUI Efficiency Nodes",
        "efficiency_optimization": true,
        "memory_optimized": true
      },
      "workflow_metadata": {
        "source": "ComfyUI workflow",
        "parser_priority": 155,
        "confidence": "high",
        "efficiency_nodes": true
      },
      "raw_workflow": "$raw_workflow"
    }
  },
  "error_handling": {
    "on_parse_failure": "return_partial_data",
    "log_errors": true,
    "validation_level": "strict"
  },
  "notes": [
    "Generic parser for ANY ComfyUI workflow using Efficiency Nodes",
    "Not limited to IllustriousXL - works with SD1.5, SDXL, FLUX, etc.",
    "Handles Efficient Loader, KSampler (Efficient), TSC nodes",
    "Priority 155 puts it above general ComfyUI parsers",
    "Uses 3-tier smart extraction for prompts and parameters"
  ],
  "enhanced_with_node_discovery": true,
  "enhancement_date": "2025-11-11"
}
