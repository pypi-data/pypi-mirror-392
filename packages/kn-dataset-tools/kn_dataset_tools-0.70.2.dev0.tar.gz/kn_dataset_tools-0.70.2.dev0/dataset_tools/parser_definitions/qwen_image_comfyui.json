{
  "parser_name": "Qwen_Image_ComfyUI",
  "priority": 190,
  "description": "Parses ComfyUI workflows created for Qwen Image models. This parser identifies specific Qwen model files and custom nodes.",
  "version": "1.0",
  "maintainer": "Gemini",
  "target_file_types": ["PNG", "JPEG", "WEBP"],
  "detection_rules": [
    {
      "comment": "Rule 1: Must be a valid ComfyUI workflow",
      "source_type": "any_metadata_source",
      "operator": "is_valid_json"
    },
    {
      "comment": "Rule 2: Must contain Qwen specific model names or nodes",
      "source_type": "any_metadata_source",
      "operator": "regex_match_any",
      "regex_patterns": [
        "qwen_image_fp8_e4m3fn.safetensors",
        "qwen_2.5_vl_7b_fp8_scaled.safetensors"
      ]
    }
  ],
  "parsing_instructions": {
    "input_data": {
      "source_options": [
        { "source_type": "exif_user_comment" },
        { "source_type": "pil_info_key", "source_key": "workflow" },
        { "source_type": "pil_info_key", "source_key": "prompt" }
      ],
      "transformations": [
        { "type": "json_decode_string_itself" }
      ]
    },
    "fields": [
      {
        "comment": "Define sampler node types variable",
        "target_key": "_sampler_node_types_var",
        "method": "static_value",
        "value": ["KSampler", "KSamplerAdvanced", "SamplerCustom", "SamplerCustomAdvanced", "ModelSamplingAuraFlow", "BasicGuider"]
      },
      {
        "comment": "BEST METHOD: Use the high-level dispatcher for prompts.",
        "target_key": "prompt",
        "method": "comfyui_extract_prompt_from_workflow",
        "text_encoder_node_types": [
          "CLIPTextEncode",
          "PCLazyTextEncode",
          "T5TextEncode",
          "PixArtT5TextEncode"
        ]
      },
      {
        "target_key": "negative_prompt",
        "method": "comfyui_extract_negative_prompt_from_workflow",
        "text_encoder_node_types": [
          "CLIPTextEncode",
          "PCLazyTextEncode",
          "T5TextEncode",
          "PixArtT5TextEncode"
        ]
      },
      {
        "target_key": "parameters.seed",
        "method": "comfy_find_input_of_main_sampler",
        "sampler_node_types": "$_sampler_node_types_var",
        "input_name": "seed"
      },
      {
        "target_key": "parameters.steps",
        "method": "comfy_find_input_of_main_sampler",
        "sampler_node_types": "$_sampler_node_types_var",
        "input_name": "steps"
      },
      {
        "target_key": "parameters.cfg_scale",
        "method": "comfy_find_input_of_main_sampler",
        "sampler_node_types": "$_sampler_node_types_var",
        "input_name": "cfg"
      },
      {
        "target_key": "parameters.sampler_name",
        "method": "comfy_find_input_of_main_sampler",
        "sampler_node_types": "$_sampler_node_types_var",
        "input_name": "sampler_name"
      },
      {
        "target_key": "parameters.scheduler",
        "method": "comfy_find_input_of_main_sampler",
        "sampler_node_types": "$_sampler_node_types_var",
        "input_name": "scheduler"
      },
      {
        "comment": "ROBUST GENERIC FINDER: Use comfy_get_node_by_class for disconnected data.",
        "target_key": "parameters.model",
        "method": "comfy_get_node_by_class",
        "class_type": "UNETLoader",
        "field_name": "unet_name"
      },
      {
        "target_key": "parameters.width",
        "method": "comfy_get_node_by_class",
        "class_type": "EmptyLatentImage",
        "field_name": "width"
      },
      {
        "target_key": "parameters.height",
        "method": "comfy_get_node_by_class",
        "class_type": "EmptyLatentImage",
        "field_name": "height"
      },
      {
        "target_key": "parameters.vae",
        "method": "comfy_get_node_by_class",
        "class_type": "VAEAutoencoderLoader",
        "field_name": "vae_name"
      },
      {
        "target_key": "raw_metadata",
        "method": "comfyui_extract_raw_workflow"
      }
    ],
    "output_template": {
      "tool": "Qwen Image (ComfyUI)",
      "prompt": "$prompt",
      "negative_prompt": "$negative_prompt",
      "parameters": {
        "model": "$parameters.model",
        "steps": "$parameters.steps",
        "cfg_scale": "$parameters.cfg_scale",
        "seed": "$parameters.seed",
        "sampler_name": "$parameters.sampler_name",
        "scheduler": "$parameters.scheduler",
        "width": "$parameters.width",
        "height": "$parameters.height",
        "vae": "$parameters.vae"
      },
      "raw_metadata": "$raw_metadata"
    }
  }
}
