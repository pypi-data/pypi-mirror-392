{
  "parser_name": "ComfyUI Modern Architectures (Unified)",
  "priority": 120,
  "description": "A consolidated parser for modern ComfyUI workflows, including T5, DiT, and advanced sampling architectures (e.g., Flux, PixArt, SD3, AuraFlow, Qwen, HiDream).",
  "version": "2.3-log-aligned",
  "maintainer": "Ktiseos Nyx & Gemini",
  "target_file_types": [ "PNG", "JPG", "JPEG", "WEBP" ],

  "detection_rules": [
    {
      "comment": "Rule 1: A valid ComfyUI JSON workflow MUST exist in the pil_info block.",
      "condition": "OR",
      "rules": [
        { "source_type": "pil_info_key", "source_key": "workflow", "operator": "is_valid_json" },
        { "source_type": "pil_info_key", "source_key": "prompt", "operator": "is_valid_json" }
      ]
    },
    {
      "comment": "Rule 2: That workflow MUST contain a node indicating a modern architecture.",
      "source_type": "pil_info_key",
      "source_key_options": [ "workflow", "prompt" ],
      "json_query_type": "has_any_node_class_type",
      "operator": "is_true",
      "class_types_to_check": [
        "DualCLIPLoader", "TripleCLIPLoader", "DualCLIPLoaderGGUF", "QuadrupleCLIPLoader",
        "T5TextEncode", "CLIPTextEncodeFlux", "PixArtT5TextEncode", "CLIPTextEncodeSD3", "TextEncodeQwenImageEdit",
        "ModelSamplingFlux", "FluxGuidance", "BasicGuider", "ModelSamplingAuraFlow",
        "PixArtSampler", "PixArtSigmaSampler", "PixArtAlphaSampler", "HiDreamSampler",
        "SamplerCustomAdvanced", "UNETLoader", "UnetLoaderGGUF", "PixArtCheckpointLoader", "Power Lora Loader (rgthree)"
      ]
    },
    {
      "comment": "Rule 3: EXCLUSION - Must NOT contain Griptape AI framework nodes (let Griptape parser handle those)",
      "operator": "NOT",
      "rules": [
        {
          "source_type": "pil_info_key",
          "source_key_options": [ "workflow", "prompt" ],
          "json_query_type": "has_any_node_class_type",
          "operator": "is_true",
          "class_types_to_check": [
            "Griptape Display: Text",
            "Griptape Create: Agent",
            "Griptape Run: Agent",
            "Griptape Agent Config: Custom Structure",
            "Griptape Create: Rules"
          ]
        }
      ]
    }
  ],

  "parsing_instructions": {
    "input_data": {
      "comment": "Based on logs, data is reliably in pil_info.",
      "source_options": [
        { "source_type": "pil_info_key", "source_key": "workflow" },
        { "source_type": "pil_info_key", "source_key": "prompt" }
      ],
      "transformations": [{ "type": "json_decode_string_itself" }]
    },
"fields": [
  {
    "target_key": "prompt",
    "method": "comfyui_targeted_prompt_extraction",
    "target_input": "positive"
  },
  {
    "target_key": "negative_prompt",
    "method": "comfyui_targeted_prompt_extraction",
    "target_input": "negative"
  },
      {
        "comment": "Define a variable for all possible sampler/guider nodes.",
        "target_key": "_sampler_node_types_var",
        "method": "static_value",
        "value": [ "KSampler", "KSamplerAdvanced", "SamplerCustom", "SamplerCustomAdvanced", "ModelSamplingAuraFlow", "BasicGuider", "PixArtSampler", "PixArtSigmaSampler", "PixArtAlphaSampler", "HiDreamSampler", "KSamplerSelect" ]
      },
      {
        "target_key": "parameters.seed",
        "method": "comfy_find_input_of_main_sampler",
        "sampler_node_types": "$_sampler_node_types_var",
        "input_name": "seed"
      },
      {
        "target_key": "parameters.steps",
        "method": "comfy_find_input_of_main_sampler",
        "sampler_node_types": "$_sampler_node_types_var",
        "input_name": "steps"
      },
      {
        "target_key": "parameters.cfg_scale",
        "method": "comfy_find_input_of_main_sampler",
        "sampler_node_types": "$_sampler_node_types_var",
        "input_name": "cfg"
      },
      {
        "target_key": "parameters.sampler_name",
        "method": "comfy_find_input_of_main_sampler",
        "sampler_node_types": "$_sampler_node_types_var",
        "input_name": "sampler_name"
      },
      {
        "target_key": "parameters.scheduler",
        "method": "comfy_find_input_of_main_sampler",
        "sampler_node_types": "$_sampler_node_types_var",
        "input_name": "scheduler"
      },
      {
        "comment": "ROBUST GENERIC FINDER: Use comfy_get_node_by_class for disconnected data.",
        "target_key": "parameters.model",
        "method": "comfy_get_node_by_class",
        "class_type": "UNETLoader",
        "field_name": "unet_name"
      },
      {
        "target_key": "parameters.width",
        "method": "comfy_get_node_by_class",
        "class_type": "EmptyLatentImage",
        "field_name": "width"
      },
      {
        "target_key": "parameters.height",
        "method": "comfy_get_node_by_class",
        "class_type": "EmptyLatentImage",
        "field_name": "height"
      },
      {
        "target_key": "raw_metadata",
        "method": "comfyui_extract_raw_workflow"
      }
    ],
    "output_template": {
      "tool": "ComfyUI (Modern Architecture)",
      "parser_version": "Unified v2.3",
      "prompt": "$prompt",
      "negative_prompt": "$negative_prompt",
      "parameters": "$parameters",
      "raw_metadata": "$raw_metadata"
    }
  }
}
