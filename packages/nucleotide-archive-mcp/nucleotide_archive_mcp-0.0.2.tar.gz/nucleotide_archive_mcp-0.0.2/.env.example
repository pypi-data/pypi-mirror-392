# RNA Dataset Search MCP Server Configuration
# Copy this file to .env and customize as needed

# ENA API Configuration
# Override API base URLs (useful for testing or custom mirrors)
# ENA_PORTAL_API_BASE=https://www.ebi.ac.uk/ena/portal/api
# ENA_BROWSER_API_BASE=https://www.ebi.ac.uk/ena/browser/api

# Request timeout in seconds (default: 30.0)
# ENA_TIMEOUT=30.0

# Default search result limit (default: 20)
# ENA_SEARCH_LIMIT=20

# Rate limiting - maximum requests per second (default: 10.0)
# ENA_MAX_RPS=10.0

# Logging level: DEBUG, INFO, WARNING, ERROR, CRITICAL (default: INFO)
# LOG_LEVEL=INFO
