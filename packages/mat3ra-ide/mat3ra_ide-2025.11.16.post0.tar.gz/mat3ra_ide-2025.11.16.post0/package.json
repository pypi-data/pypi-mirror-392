{
    "name": "@exabyte-io/ide.js",
    "version": "0.0.0",
    "description": "Infrastructure DEfinitions",
    "scripts": {
        "test": "nyc --reporter=text mocha --recursive --bail",
        "lint": "eslint --cache src/js tests/js && prettier --write src/js tests/js",
        "lint:fix": "eslint --fix --cache src/js tests/js && prettier --write src/js tests/js",
        "lint:staged": "lint-staged",
        "prepare": "husky install",
        "prettier": "prettier --check src/js tests/js",
        "transpile": "babel --out-dir dist/js src/js",
        "postinstall": "npm run transpile"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Exabyte-io/ide.js.git"
    },
    "main": "dist/js/index.js",
    "files": [
        "/dist",
        "/src/js",
        ".babelrc"
    ],
    "author": "Exabyte Inc.",
    "bugs": {
        "url": "https://github.com/Exabyte-io/ide.js/issues"
    },
    "license": "Apache-2.0",
    "homepage": "https://github.com/Exabyte-io/ide.js",
    "dependencies": {
        "@babel/cli": "7.16.0",
        "@babel/core": "7.16.0",
        "@babel/eslint-parser": "7.16.3",
        "@babel/plugin-proposal-class-properties": "7.16.0",
        "@babel/preset-env": "7.16.4",
        "@babel/preset-react": "7.16.7",
        "@babel/register": "^7.16.0",
        "@babel/runtime-corejs3": "7.16.8",
        "chai": "^4.3.4",
        "lodash": "^4.17.21",
        "moment": "^2.17.1",
        "pluralize": "^3.0.0"
    },
    "devDependencies": {
        "@mat3ra/code": "2024.3.25-3",
        "@mat3ra/esse": "2024.3.25-6",
        "@exabyte-io/eslint-config": "^2022.11.17-0",
        "chai": "^4.3.4",
        "eslint": "7.32.0",
        "eslint-config-airbnb": "19.0.2",
        "eslint-config-prettier": "^8.5.0",
        "eslint-import-resolver-exports": "^1.0.0-beta.2",
        "eslint-plugin-import": "2.25.3",
        "eslint-plugin-jsdoc": "37.1.0",
        "eslint-plugin-jsx-a11y": "6.5.1",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-react": "7.30.0",
        "eslint-plugin-simple-import-sort": "7.0.0",
        "husky": "^7.0.4",
        "lint-staged": "^12.1.2",
        "mixwith": "^0.1.1",
        "mocha": "^9.1.3",
        "nyc": "^15.1.0",
        "prettier": "^2.7.1"
    },
    "peerDependencies": {
        "@mat3ra/code": "*",
        "@mat3ra/esse": "*"
    },
    "engines": {
        "node": ">=14.0.0"
    },
    "lint-staged": {
        "src/js/**/*.js": [
            "eslint --fix --cache",
            "prettier --write"
        ],
        "tests/js/**/*.js": [
            "eslint --fix --cache",
            "prettier --write"
        ]
    }
}
