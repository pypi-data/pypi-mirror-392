[0.019406, "o", "\u001b[H\u001b[2J"]
[0.000538, "o", "SpecQL Reverse Engineering Demo\r\n"]
[0.000086, "o", "================================\r\n\r\n"]
[2.005308, "o", "Scenario: Converting existing PostgreSQL schema to SpecQL YAML\r\n"]
[0.000105, "o", "\r\n"]
[2.003688, "o", "Existing PostgreSQL schema:\r\n\r\n"]
[0.003567, "o", "CREATE TABLE crm.tb_contact (\r\n    pk_contact SERIAL PRIMARY KEY,\r\n    id UUID DEFAULT gen_random_uuid() UNIQUE,\r\n    identifier TEXT,\r\n    first_name TEXT,\r\n    last_name TEXT,\r\n    email TEXT,\r\n    created_at TIMESTAMP DEFAULT NOW()\r\n);\r\n"]
[4.004608, "o", "\r\nRunning: specql reverse postgresql schema.sql --output entities/\r\n\r\n"]
[2.004308, "o", "Generated SpecQL YAML:\r\n\r\n"]
[0.003236, "o", "entity: Contact\r\nschema: crm\r\n\r\nfields:\r\n  first_name: text\r\n  last_name: text\r\n  email: email\r\n  created_at: timestamp\r\n\r\nindexes:\r\n  - fields: [email]\r\n    unique: true\r\n"]
[4.004692, "o", "\r\nâœ… Reverse engineering complete!\r\n\r\n"]
[0.000069, "o", "Now you can:\r\n"]
[0.000097, "o", "  - Edit the YAML to add business logic\r\n"]
[0.000204, "o", "  - Generate code for multiple languages\r\n  - Maintain schema as YAML going forward\r\n\r\n"]
[3.003901, "x", "0"]
