{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "core:webview:allow-internal-toggle-devtools"
  ],
  "remote": {
    "urls": ["http://localhost:*", "http://127.0.0.1:*"]
  }
}
