{
  "menu": {
    "file_new": {
      "keys": ["Ctrl+N"],
      "primary": "Ctrl+N",
      "description": "New file"
    },
    "file_open": {
      "keys": ["Ctrl+O"],
      "primary": "Ctrl+O",
      "description": "Open file"
    },
    "file_save": {
      "keys": ["Ctrl+S"],
      "primary": "Ctrl+S",
      "description": "Save file"
    },
    "file_save_as": {
      "keys": ["Ctrl+Shift+S"],
      "primary": "Ctrl+Shift+S",
      "description": "Save file as"
    },
    "file_quit": {
      "keys": ["Ctrl+Q"],
      "primary": "Ctrl+Q",
      "description": "Quit application"
    },
    "run_program": {
      "keys": ["Ctrl+R"],
      "primary": "Ctrl+R",
      "description": "Run program"
    },
    "help_topics": {
      "keys": ["Ctrl+?", "Ctrl+/"],
      "primary": "Ctrl+?",
      "description": "Open help topics"
    }
  },
  "editor": {
    "cut": {
      "keys": ["Ctrl+X"],
      "primary": "Ctrl+X",
      "description": "Cut selected text"
    },
    "copy": {
      "keys": ["Ctrl+C"],
      "primary": "Ctrl+C",
      "description": "Copy selected text"
    },
    "paste": {
      "keys": ["Ctrl+V"],
      "primary": "Ctrl+V",
      "description": "Paste text"
    },
    "find": {
      "keys": ["Ctrl+F"],
      "primary": "Ctrl+F",
      "description": "Find text"
    },
    "replace": {
      "keys": ["Ctrl+H"],
      "primary": "Ctrl+H",
      "description": "Replace text"
    },
    "smart_insert": {
      "keys": ["Ctrl+I"],
      "primary": "Ctrl+I",
      "description": "Smart insert line between current and next"
    },
    "toggle_breakpoint": {
      "keys": ["Ctrl+B"],
      "primary": "Ctrl+B",
      "description": "Toggle breakpoint on current line"
    },
    "renumber": {
      "keys": ["Ctrl+E"],
      "primary": "Ctrl+E",
      "description": "Renumber all lines"
    }
  },
  "view": {
    "toggle_variables": {
      "keys": ["Ctrl+W"],
      "primary": "Ctrl+W",
      "description": "Toggle variables window"
    },
    "toggle_stack": {
      "keys": ["Ctrl+K"],
      "primary": "Ctrl+K",
      "description": "Toggle execution stack window"
    }
  },
  "help_browser": {
    "search": {
      "keys": ["Return"],
      "primary": "Return",
      "description": "Execute search (when in search box)"
    },
    "inpage_search": {
      "keys": ["Ctrl+F"],
      "primary": "Ctrl+F",
      "description": "Find text in current help page"
    }
  }
}
