{
  "editor": {
    "run": {
      "keys": ["Ctrl+R", "F5"],
      "primary": "Ctrl+R/F5",
      "description": "Run program"
    },
    "stop": {
      "keys": ["Esc"],
      "primary": "Esc",
      "description": "Stop program execution"
    },
    "save": {
      "keys": ["Ctrl+S"],
      "primary": "Ctrl+S",
      "description": "Save program"
    },
    "new": {
      "keys": ["Ctrl+N"],
      "primary": "Ctrl+N",
      "description": "New program"
    },
    "open": {
      "keys": ["Ctrl+O"],
      "primary": "Ctrl+O",
      "description": "Open file"
    },
    "help": {
      "keys": ["F1"],
      "primary": "F1",
      "description": "Open help"
    },
    "toggle_breakpoint": {
      "keys": ["F9"],
      "primary": "F9",
      "description": "Toggle breakpoint"
    },
    "step": {
      "keys": ["F10"],
      "primary": "F10",
      "description": "Step to next line"
    },
    "continue": {
      "keys": ["F5"],
      "primary": "F5",
      "description": "Continue execution"
    },
    "toggle_variables": {
      "keys": ["Ctrl+Alt+V"],
      "primary": "Ctrl+Alt+V",
      "description": "Toggle variables window"
    }
  }
}
