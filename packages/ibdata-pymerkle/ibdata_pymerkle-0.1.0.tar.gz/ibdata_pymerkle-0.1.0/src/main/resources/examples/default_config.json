{
  "algorithm": "sha256",
  "merkle_filename": ".ibdata.merkle",
  "include_hidden": false,
  "follow_symlinks": false,
  "exclude_patterns": [
    "*.tmp",
    "*.log",
    "__pycache__",
    "*.pyc",
    ".git",
    ".DS_Store",
    "Thumbs.db"
  ],
  "max_file_size": null,
  "create_subdirectory_merkles": true,
  "include_metadata": false,
  "chunk_size": 8192
}