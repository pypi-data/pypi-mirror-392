<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="license_view_search" model="ir.ui.view">
        <field name="name">license_partner_commission.license_view_search</field>
        <field name="model">license.license</field>
        <field name="inherit_id" ref="license.license_view_search" />
        <field name="arch" type="xml">
            <xpath expr="//field[@name='partner_id']" position="after">
                <filter
                    string="Referrer"
                    context="{'group_by': 'referrer_id'}"
                    name="group_by_referrer"
                />
            </xpath>
        </field>
    </record>
</odoo>
