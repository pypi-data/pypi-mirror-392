{
  "name": "three-jupyterlab",
  "version": "1.0.0",
  "description": "JupyterLab extension with custom UI for Python code execution",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "license": "MIT",
  "author": "",
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "repository": {
    "type": "git",
    "url": ""
  },
  "scripts": {
    "build": "npm run build:lib && npm run build:labextension:dev",
    "build:prod": "npm run clean && npm run build:lib:prod && npm run build:labextension",
    "build:labextension": "jupyter labextension build .",
    "build:labextension:dev": "jupyter labextension build . --dev-build",
    "build:lib": "tsc",
    "build:lib:prod": "tsc -p tsconfig.prod.json",
    "clean": "npm run clean:lib",
    "clean:lib": "rimraf lib tsconfig.tsbuildinfo",
    "clean:labextension": "rimraf three_jupyterlab/labextension",
    "clean:all": "npm run clean:lib && npm run clean:labextension",
    "eslint": "eslint . --ext .ts,.tsx --fix",
    "eslint:check": "eslint . --ext .ts,.tsx",
    "prepack": "npm run clean && npm run build:prod",
    "watch": "tsc -w",
    "dev": "npm run build && jupyter labextension develop --overwrite . && start jupyter lab",
    "dev:quick": "npm run build && start jupyter lab",
    "start": "npm run build && jupyter labextension install . && start jupyter lab"
  },
  "dependencies": {
    "@jupyterlab/application": "^4.0.0",
    "@jupyterlab/apputils": "^4.0.0",
    "@jupyterlab/coreutils": "^6.0.0",
    "@jupyterlab/docregistry": "^4.0.0",
    "@jupyterlab/launcher": "^4.0.0",
    "@jupyterlab/notebook": "^4.0.0",
    "@jupyterlab/services": "^7.0.0",
    "@jupyterlab/ui-components": "^4.0.0",
    "@lumino/coreutils": "^2.0.0",
    "@lumino/disposable": "^2.0.0",
    "@lumino/signaling": "^2.0.0",
    "@lumino/widgets": "^2.0.0",
    "marked": "^11.0.0",
    "monaco-editor": "^0.45.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "three": "^0.160.0"
  },
  "devDependencies": {
    "@jupyterlab/builder": "^4.0.0",
    "@types/marked": "^6.0.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/three": "^0.160.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.0",
    "typescript": "~5.2.0"
  },
  "jupyterlab": {
    "extension": true,
    "outputDir": "three_jupyterlab/labextension",
    "_build": {
      "load": "static\\remoteEntry.2d72eac67e2a3f32624f.js",
      "extension": "./extension",
      "style": "./style"
    }
  }
}
