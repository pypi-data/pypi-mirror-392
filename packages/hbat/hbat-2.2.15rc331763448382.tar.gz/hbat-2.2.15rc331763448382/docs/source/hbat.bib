	journal = {Journal of Chemical Theory and Computation},
	author = {Xiao, Yao and Woods, Robert J.},
	month = aug,
	year = {2023},
	note = {Publisher: American Chemical Society},
	pages = {5503--5515},
	file = {Full Text PDF:/Users/abhishektiwari/Zotero/storage/IB4EE72N/Xiao and Woods - 2023 - Protein-Ligand CH-œÄ Interactions Structural Informatics, Energy Function Development, and Docking I.pdf:application/pdf},
}

@article{wang_dissecting_2019,
	title = {Dissecting {C}-{H}‚àô‚àô‚àôœÄ and {N}-{H}‚àô‚àô‚àôœÄ {Interactions} in {Two} {Proteins} {Using} a {Combined} {Experimental} and {Computational} {Approach}},
	volume = {9},
	copyright = {2019 The Author(s)},
	issn = {2045-2322},
	url = {https://www.nature.com/articles/s41598-019-56607-4},
	doi = {10.1038/s41598-019-56607-4},
	abstract = {C-H‚àô‚àô‚àôœÄ and N-H‚àô‚àô‚àôœÄ interactions can have an important contribution for protein stability. However, direct measurements of these interactions in proteins are rarely reported. In this work, we combined the mutant cycle experiments and molecular dynamics (MD) simulations to characterize C-H‚àô‚àô‚àôœÄ and N-H‚àô‚àô‚àôœÄ interactions and their cooperativity in two model proteins. It is shown that the average C-H‚àô‚àô‚àôœÄ interaction per residue pair is {\textasciitilde} -0.5kcal/mol while the N-H‚àô‚àô‚àôœÄ interaction is slightly stronger. The triple mutant box measurement indicates that N-H‚àô‚àô‚àôœÄ‚àô‚àô‚àôC-H‚àô‚àô‚àôœÄ and C-H‚àô‚àô‚àôœÄ‚àô‚àô‚àôC-H‚àô‚àô‚àôœÄ can have a positive or negative cooperativity. MD simulations suggest that the cooperativity, depending on the local environment of the interactions, mainly arises from the geometric rearrangement when the nearby interaction is perturbed.},
	language = {en},
	number = {1},
	urldate = {2025-08-08},
	journal = {Scientific Reports},
