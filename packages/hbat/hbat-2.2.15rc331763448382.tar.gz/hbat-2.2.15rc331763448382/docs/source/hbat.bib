	volume = {307},
	issn = {0022-2836},
	url = {https://www.sciencedirect.com/science/article/pii/S0022283600944735},
	doi = {10.1006/jmbi.2000.4473},
	abstract = {A non-redundant set of 1154 protein structures from the Protein Data Bank was examined with respect to close interactions between C-H-donor and œÄ-acceptor groups. A total of 31,087 interactions were found to satisfy our selection criteria. Their geometric parameters suggest that these interactions can be classified as weak hydrogen bonds. A set of 12 interaction classes were defined based on the division of the donors into three groups and the acceptors into four groups. These classes were examined separately, and the respective interactions described in detail in each class. Most prominent were interactions between aliphatic C-H donors and aromatic œÄ-acceptors and interactions between aromatic C-H donors and aromatic œÄ-acceptors. About three-quarters of the Trp-rings, half of all Phe and Tyr-rings and a quarter of all His-rings were found to be involved as acceptors in C-H‚ãØœÄ-interactions. On the donor side, a preference for aromatic C-H groups was observed, but also for the aliphatic side-chains of the long, extended amino acid residues Lys, Arg and Met, and the Pro ring. The average distance between the C-donor and the center-of-mass of the œÄ-acceptor was observed to be significantly longer in the 174 protein structures determined at {\textgreater}2.5 √Ö resolution. Also, the distribution is significantly wider. This resolution dependance suggests that the force fields commonly used for the refinement of protein structures may not be adequate. C-H‚ãØœÄ-interactions involving aromatic groups either as donor or as acceptor groups are found mostly in the interior of the protein. The more hydrophilic the participating groups are, the closer to the surface are the interactions located. About 40 \% of all C-H‚ãØœÄ-interactions occur between amino acid residue side-chains that are separated by nine or less residues in sequence. Dependent on the interaction class, different preferences for secondary structure, residue type and side-chain conformation were observed. It is likely that the C-H‚ãØœÄ-interactions contribute significantly to the overall stability of a protein.},
	number = {1},
	urldate = {2025-08-08},
	journal = {Journal of Molecular Biology},
	author = {Brandl, Maria and Weiss, Manfred S. and Jabs, Andreas and S√ºhnel, J√ºrgen and Hilgenfeld, Rolf},
	month = mar,
	year = {2001},
	keywords = {C-H‚ãØœÄ-interaction, force fields, hydrogen bond, protein stability, protein structure},
	pages = {357--377},
	file = {ScienceDirect Snapshot:/Users/abhishektiwari/Zotero/storage/3GV4HHUN/S0022283600944735.html:text/html},
}

@article{xiao_proteinligand_2023,
	title = {Protein-{Ligand} {CH}-œÄ {Interactions}: {Structural} {Informatics}, {Energy} {Function} {Development}, and {Docking} {Implementation}},
