@article{jubb_arpeggio_2017,
	series = {Computation {Resources} for {Molecular} {Biology}},
	title = {Arpeggio: {A} {Web} {Server} for {Calculating} and {Visualising} {Interatomic} {Interactions} in {Protein} {Structures}},
