{
  "description": "Incoming chat payload for both CLI and FastAPI surfaces.",
  "properties": {
    "date_from": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Date From"
    },
    "date_to": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Date To"
    },
    "feedback": {
      "anyOf": [
        {
          "enum": [
            "up",
            "down"
          ],
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Feedback"
    },
    "question": {
      "minLength": 1,
      "title": "Question",
      "type": "string"
    },
    "save": {
      "default": true,
      "title": "Save",
      "type": "boolean"
    },
    "session_id": {
      "anyOf": [
        {
          "pattern": "^[A-Za-z0-9_.\\-]+$",
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Session Id"
    },
    "source": {
      "anyOf": [
        {
          "items": {
            "type": "string"
          },
          "type": "array"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Source"
    },
    "tags": {
      "anyOf": [
        {
          "items": {
            "type": "string"
          },
          "type": "array"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Tags"
    },
    "top": {
      "anyOf": [
        {
          "minimum": 1,
          "type": "integer"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Top"
    }
  },
  "required": [
    "question"
  ],
  "title": "ChatRequest",
  "type": "object"
}
