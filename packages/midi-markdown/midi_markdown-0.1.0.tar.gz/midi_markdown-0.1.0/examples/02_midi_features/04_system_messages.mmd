# System Messages and SysEx
# Demonstrates system common, real-time, and SysEx messages

# Setup
[00:00.000]
- tempo 120
- time_signature 4/4
- marker "System Messages Demo"

# ============================================
# System Exclusive (SysEx)
# ============================================

[00:00.000]
- marker "SysEx Examples"

# Roland device identity request
[@]
- sysex F0 41 10 00 11 F7

# Yamaha device parameter change
[00:01.000]
- sysex F0 43 10 4C 00 00 7E 00 F7

# Generic manufacturer SysEx
[00:02.000]
- sysex F0 7E 00 06 01 F7

# Multi-line SysEx (for longer messages)
[00:02.500]
- sysex F0 00 01 06
        02 03 04 05
        06 07 08 09
        0A 0B 0C F7

# ============================================
# System Common Messages
# ============================================

[00:03.000]
- marker "System Common"

# MIDI Time Code Quarter Frame
[@]
- mtc_quarter_frame 0

[00:04.000]
- mtc_quarter_frame 64

# Song Position Pointer
[00:05.000]
- song_position 0

[00:06.000]
- song_position 100

# Song Select
[00:07.000]
- song_select 5

# Tune Request
[00:08.000]
- tune_request

# ============================================
# System Real-Time Messages
# ============================================

[00:09.000]
- marker "Real-Time"

# Timing Clock
[@]
- clock_tick

# Start
[00:10.000]
- clock_start

# Continue
[00:11.000]
- clock_continue

# Stop
[00:12.000]
- clock_stop

# Active Sensing
[00:13.000]
- active_sensing

# System Reset
[00:14.000]
- system_reset

# ============================================
# Channel Mode Messages
# ============================================

[00:15.000]
- marker "Channel Mode"

